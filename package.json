    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
