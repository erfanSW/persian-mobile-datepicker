    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^6.2.9",
