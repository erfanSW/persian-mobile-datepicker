{
  "name": "@persian-tools/persian-mobile-datepicker",
  "version": "0.0.1",
  "description": "Modern Persian Mobile date picker",
  "publishConfig": {
    "access": "public"
  },
  "source": "src/index.tsx",
  "main": "build/index.js",
  "umd:main": "build/index.umd.js",
  "esmodule": "build/index.modern.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "base": "microbundle --no-css-modules --compress --name persian-mobile-datepicker --strict --jsx React.createElement",
    "build": "npm run base -- --raw --sourcemap false",
    "dev": "npm run base -- watch",
    "test": "jest --coverage --config=jest.config.js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/persian-tools/persian-mobile-datepicker.git"
  },
  "keywords": [
    "persian",
    "datepicker",
    "mobile-datepicker",
    "persian-datepicker"
  ],
  "files": [
    "build"
  ],
  "engines": {
    "node": ">= 12.13.0"
  },
  "author": "Ali Torki <ali_4286@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/persian-tools/persian-mobile-datepicker/issues"
